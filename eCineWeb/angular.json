{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "eCineWeb": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/eCineWeb",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/vendor/sweetalert/lib/sweet-alert.css",
              "src/assets/vendor/fontawesome/css/font-awesome.css",
              "src/assets/vendor/metisMenu/dist/metisMenu.css",
              "src/assets/vendor/animate.css/animate.css",
              "src/assets/vendor/bootstrap/dist/css/bootstrap.css",
              "src/assets/fonts/pe-icon-7-stroke/css/pe-icon-7-stroke.css",
              "src/assets/fonts/pe-icon-7-stroke/css/helper.css",
              "src/assets/styles/style.css",
              "src/assets/vendor/select2-3.5.2/select2.css",
              "src/assets/vendor/select2-bootstrap/select2-bootstrap.css",
              "src/assets/vendor/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css"
            ],
            "scripts": [
              "src/assets/vendor/jquery/dist/jquery.min.js",
              "src/assets/vendor/jquery-ui/jquery-ui.min.js",
              "src/assets/vendor/slimScroll/jquery.slimscroll.min.js",
              "src/assets/vendor/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/vendor/jquery-flot/jquery.flot.js",
              "src/assets/vendor/jquery-flot/jquery.flot.resize.js",
              "src/assets/vendor/jquery-flot/jquery.flot.pie.js",
              "src/assets/vendor/flot.curvedlines/curvedLines.js",
              "src/assets/vendor/jquery.flot.spline/index.js",
              "src/assets/vendor/metisMenu/dist/metisMenu.min.js",
              "src/assets/vendor/iCheck/icheck.min.js",
              "src/assets/vendor/peity/jquery.peity.min.js",
              "src/assets/vendor/sparkline/index.js",
              "src/assets/scripts/homer.js",
              "src/assets/vendor/select2-3.5.2/select2.min.js",
              "src/assets/vendor/xeditable/bootstrap3-editable/js/bootstrap-editable.min.js",
              "src/assets/vendor/iCheck/icheck.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "eCineWeb:build:production"
            },
            "development": {
              "browserTarget": "eCineWeb:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eCineWeb:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "eCineWeb"
}
